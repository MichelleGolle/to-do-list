<table class="table table-bordered">
  <thead>
    <tr>
      <th class="task-title">Title</th>
      <th>Description</th>
      <th>Due Date</th>
    </tr>
  </thead>
  <tbody>

    <tr class="task"><% @complete_tasks.each do |task| %>
      <td><%= task.title %></td>
      <td><%= task.description %></td>
      <td><%= task.due_date %></td>
      <td><%= link_to "Edit", edit_list_task_path(id: task.id) %></td>
      <td><%= link_to "Delete", list_task_path(id: task.id) %></td></tr>
    <% end %>



  </tbody>
</table>
</div>

</body>
</html>

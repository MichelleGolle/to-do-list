<div class="user-header"><h1><%= current_user.username %>'s task lists:</h1></div>
<%= link_to "Create new list", new_list_path %>

<% @lists.each do |list| %>
<%= link_to "#{list.title}",list_tasks_path(list_id: list.id) %>
<%= link_to "Edit", edit_list_path(list) %>
<%= link_to "Delete", list_path(list), method: "delete" %>
<% end %>
